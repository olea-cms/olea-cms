doctype html
head
  meta(charset='UTF-8')
  link(rel='icon' type='image/svg+xml' href='https://vitejs.dev/logo.svg')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Vite App
.m-auto(x-data='{ count: 0 }')
  .flex.gap-2
    a(href='https://vitejs.dev' target='_blank')
      img.h-10(src='https://vitejs.dev/logo.svg' alt='Vite logo')
    a(href='https://developer.mozilla.org/en-US/docs/Web/JavaScript' target='_blank')
      img.h-10(src='data:image/png;base64,' alt='JavaScript logo')
    a(href='https://tailwindcss.com' target='_blank')
      img.h-10(src='https://tailwindcss.com/favicons/favicon-32x32.png?v=3' alt='Vite logo')
    a(href='https://alpinejs.dev' target='_blank')
      img.h-10(src='https://alpinejs.dev/favicon.png' alt='Alpine JS logo')
  .mt-4.text-center
    span.text-red-700.text-3xl(x-text='count')
    .flex.justify-between.mt-4
      button.text-red-700.border.border-red-700.font-medium.rounded-lg.text-sm.px-2.py-1.text-center.me-2.mb-2(x-on:click='count--' class='hover:text-white hover:bg-red-800 focus:ring-4 focus:outline-none focus:ring-red-300 dark:border-red-500 dark:text-red-500 dark:hover:text-white dark:hover:bg-red-500 dark:focus:ring-red-800') Decrement
      button.text-blue-700.border.border-blue-700.font-medium.rounded-lg.text-sm.px-2.py-1.text-center.me-2.mb-2(x-on:click='count++' class='hover:text-white hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:border-blue-500 dark:text-blue-500 dark:hover:text-white dark:hover:bg-blue-500 dark:focus:ring-blue-800') Increment
script(type='module' src='src/main.ts')

